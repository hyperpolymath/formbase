{
  "$schema": "https://deno.land/x/deno/cli/schemas/config-file.v1.json",
  "compilerOptions": {
    "lib": ["deno.window", "dom", "dom.iterable"],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "imports": {
    "react": "npm:react@^18.2.0",
    "react-dom": "npm:react-dom@^18.2.0",
    "@rescript/core": "npm:@rescript/core@^1.3.0",
    "@rescript/runtime": "npm:@rescript/runtime@^12.0.2",
    "jotai": "npm:jotai@^2.6.0",
    "yjs": "npm:yjs@^13.6.0"
  },
  "tasks": {
    "setup": "echo 'Run: deno task dev'",
    "dev": "deno run -A npm:vite",
    "build": "deno run -A npm:vite build",
    "preview": "deno run -A npm:vite preview",
    "test": "deno test --allow-read --allow-env",
    "lint": "deno lint",
    "fmt": "deno fmt",
    "storybook": "deno run -A npm:storybook dev -p 6006"
  }
}
